.menu-button {
	position: relative;
	width: 36px;
	height: 24px;
	padding: 0;
	vertical-align: middle;
	background-color: transparent;
	transition-property: transform;
	transition-duration: 0.2s;

	-webkit-tap-highlight-color: transparent;

	span {
		position: absolute;
		left: 0;
		width: 100%;
		height: 4px;
		border-radius: 4px;
		background-color: #fff;
		transition-property: transform, width;
		transition-duration: 0.3s;

		&:nth-child(1) {
			top: 0;
		}

		&:nth-child(2) {
			top: 50%;
			margin-top: -2px;
		}

		&:nth-child(3) {
			bottom: 0;
			width: 67%;
		}
	}

	&.menu-button--active {

	}

	@include breakpoint(extra-small) {
		width: 22px;
		height: 17px;

		span {
			height: 3px;

			&:nth-child(2) {
				margin-top: -1.5px;
			}
		}

		&.menu-button--active {
			span {
				&:nth-child(1) {
					top: calc(50% - 3px);
					transform: rotate(45deg) translateZ(0);
				}

				&:nth-child(2) {
					opacity: 0;
				}

				&:nth-child(3) {
					bottom: 50%;
					width: 100%;
					transform: rotate(-45deg) translateZ(0);
				}
			}
		}
	}
}

