/* Simple Popup  */

.popup {
	position: fixed;
	left: 0;
	top: 0;
	z-index: 30;
	opacity: 0;
	visibility: hidden;
	display: flex;
	width: 100%;
	height: 100%;
	overflow-y: auto;
	-webkit-overflow-scrolling: touch;
	padding: 0 20px;

	transition-property: opacity, visibility;
	transition-duration: 0.4s;

	&::before {
		content: "";
		position: fixed;
		left: 0;
		top: 0;
		width: 100%;
		height: calc(100% + 200px); // need for fix issue on mobile devices
		background-color: rgba(255,255,255, 0.6);
		transform: translateY(-60px); // need for fix issue on mobile devices
	}

	.popup__container {
		max-width: 870px;
		width: 100%;
		margin: auto;
		padding: 30px 0;
	}

	.popup__content {
		position: relative;
		background-color: #fff;
		border-radius: 10px;
		padding-top: 63px;
		padding-bottom: 30px;
		padding-left: 86px;
		padding-right: 30px;
		font-size: 12px;
		letter-spacing: 0.5px;
		color: #3c3c3c;
		line-height: 2.08;
		transform: translateY(-20px);
		transition-property: transform;
		transition-duration: 0.3s;

		a {
			color: #3c3c3c;
			text-decoration: underline;

			&:hover {
				text-decoration: none;
			}
		}

		p + p {
			margin-top: 15px;
		}
	}

	// block with scroll bar
	.popup__inner {
		overflow-y: auto;
		max-height: 329px;
		padding-right: 50px;

		-webkit-overflow-scrolling: touch;

		@include scroll-bar;
	}
	// block with scroll bar end

	.popup__iframe-wrapper {
		position: relative;
		padding-top: 56.25%; // 16:9

		iframe {
			position: absolute;
			left: 0;
			top: 0;
			width: 100%;
			height: 100%;
		}
	}

	.popup__close-button {
		position: absolute;
		top: 17px;
		right: 17px;
		width: 25px;
		height: 25px;
		padding: 4px;
		background-color: transparent;
		transition-property: transform;
		transition-duration: 0.4s;

		-webkit-tap-highlight-color: transparent;

		.icon {
			width: 100%;
		}

		&:hover {
			transform: rotate(90deg);
		}
	}


	// Popup open state
	&.popup--open {
		opacity: 1;
		visibility: visible;

		.popup__content {
			transform: translateY(0);
		}
	}
	
	// Popup text using for popup with TEXT content and inner scrollbar. (full width white bg + block with scroll text)
	&.popup--text {}

	@include breakpoint(small) {
		.popup__content {
			padding-left: 25px;
			padding-right: 25px;
		}

		.popup__inner {
			max-height: none;
			padding-right: 18px;
		}
	}
	
	@include breakpoint(extra-small) {
		
		&.popup--text {
			overflow: hidden;
			padding-left: 0;
			padding-right: 0;

			&::before {
				background-color: #fff;
			}

			.popup__close-button {
				top: 20px;
				right: 16px;
			}

			.popup__container {
				margin: 0;
				padding: 0;
			}

			.popup__content {
				height: 100%;
				padding-top: 50px;
				padding-bottom: 50px;
				padding-right: 20px;
				padding-left: 20px;
			}

			.popup__inner {
				max-height: 100%;
			}
		}
	}
}
