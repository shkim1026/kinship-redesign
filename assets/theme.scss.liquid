		> .nav-menu__item {
			transition-duration: 0.15s;

			&:nth-child(1) {
				transform: translateY(10px);
			}

			+ .nav-menu__item {
				margin-top: 20px;
			}

			> .nav-menu__link {
				padding-bottom: 20px;
				font-size: 38px;
				letter-spacing: 4.63px;
				line-height: 1.15;
			}

