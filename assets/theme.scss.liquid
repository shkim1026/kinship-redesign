.featured-products {
	position: relative;
	padding-bottom: 120px;
	background-color: #ffe2b0;

	.container {
		position: relative;
	}

	.featured-products__link-wrapper {
		position: absolute;
		top: 0;
		left: 0;
		width: 100%;
		height: 100%;
	}

	.featured-products__wrapper {
		position: relative;
		z-index: 2;
		transform: translateY(-105px);

		/* width */
		&::-webkit-scrollbar {
			display: none;
			background-color: transparent;
		}
	}

	.featured-products__row {
		display: grid;
		grid-gap: 30px;
		grid-template-columns: repeat(3, minmax(320px, 1fr));

		&.featured-products__row--big-card {
			grid-template-columns: minmax(393px, 1fr) minmax(500px, 2fr);

			> div:not(.featured-products__big-card):not(.featured-products__card) {
				display: none;
			}
		}
	}

	&::before {
		content: "";
		position: absolute;
		left: 0;
		top: 0;
		height: 175px;
		width: 100%;
	}

	.featured-products__panel {
		height: 175px;
	}

	// Promo card default
	.featured-products__card {
		display: flex;
		flex-direction: column;
		justify-content: center;
		position: relative;
		padding: 80px 50px 47px 64px;
		border-radius: 10px;
		background-repeat: no-repeat;
		background-size: cover;
		background-position: 50% 50%;
		color: #ccf6ff;

		.featured-products__card-button-wrapper {
			display: flex;
			align-items: flex-end;
			flex-grow: 1;
		}

		.button {
			position: relative;
			z-index: 1;
			min-width: auto;
			max-width: 277px;
			width: 100%;
			margin-top: 30px;
		}
		
		&.featured-products__card--center {
			align-items: center;
			text-align: center;
		}
	}

	.featured-products__card-title {
		font-weight: 800;
		letter-spacing: 0.122em;
		line-height: 1.1;
		text-transform: uppercase;

		&.featured-products__card-title--mobile {
			display: none;
		}
	}

	.featured-products__card-text {
		margin-top: 20px;
		line-height: 2.13;
		font-size: 14px;
		letter-spacing: 0.09em;
	}

	.featured-products__card-link-wrapper {
		position: absolute;
		top: 0;
		left: 0;
		width: 100%;
		height: 100%;
	}

	// Promo card default end

	// Card with FULL size image
	.featured-products__image-card {
		position: relative;
		border-radius: 10px;
		overflow: hidden;
		transition-property: box-shadow;
		transition-duration: 0.4s;

		a {
			position: absolute;
			left: 0;
			top: 0;
			width: 100%;
			height: 100%;
		}

		img {
			position: absolute;
			left: 0;
			top: 0;
			display: block;
			width: 100%;
			height: 100%;
			object-fit: cover;
			object-position: 50% 50%;
		}
	}
	// Card with FULL size image end

	.featured-products__footer {
		max-width: 937px;
		margin-left: auto;
		margin-right: auto;
		//margin-top: 12px;
		margin-top: -50px;
		text-align: center;

		.button {
			margin-top: 42px;
		}
	}

	.featured-products__footer-button-wrapper {
		display: inline-block;
		position: relative;
		z-index: 2;
	}

	.featured-products__footer-text {
		color: $primary;
		font-size: 32px;
		font-weight: 800;
		letter-spacing: 0.062em;
		line-height: 1.75;

		a {
			text-decoration: none;
			color: inherit;
		}
	}

	.product-card {
		.product-card__reviews {
			display: none;
		}
	}

	.product-card.featured-products__big-card {
		.product-card__image {
			max-height: 400px;
		}
	}

	// Animations
	&.featured-products--with-animation {

		@media (min-width: 1200px) {
			.featured-products__card,
			.simple-card,
			.product-card {
				opacity: 0;
				transform: translateY(40px);
				transition-duration: 0.4s;
				transition-property: box-shadow, opacity, transform;
			}

			.featured-products__row {
				> div {
					@for $i from 1 through 3 {
						&:nth-child(#{$i}) {
							transition-delay: (0.1s * $i);
						}
					}
				}
			}
		}
	}

	&.animated {
		.featured-products__card,
		.simple-card,
		.product-card {
			opacity: 1;
			transform: translateY(0px);
		}
