.side-cart {
  position: fixed;
  right: 0;
  width: 100%;
  max-width: 610px;
  opacity: 0;
	top: 0;
	bottom: 0;
	height: 100vh;
	z-index: 99999;
  visibility: hidden;
  overflow: hidden;
  transition: opacity .5s ease, transform .5s ease, visibility 0s linear .5s;
  text-align: center;
  transform: translate3d(100%, 0, 0);
  padding: 25px 78px 35px;
  background-color: white;

  @include breakpoint(extra-small) {
    padding: 15px 18px 27px;
  }

  &.contains-errors{
    position: static !important;
    width: 100%;
    padding-bottom: 20px;
  }

  &.is-visible {
    opacity: 1;
    visibility: visible;
    transition: opacity .5s ease, transform .5s ease, visibility 0s linear 0s;
    transform: translate3d(0, 0, 0);
  }

  h3 {
    font-size: 10px;
    line-height: 1.45;
    letter-spacing: 1.6px;

    @media screen and (min-width:768px) {
      margin-bottom: 10px;
    }
  }

  p {
    text-transform: uppercase;
    font-size: 10px;
    line-height: 1.45;
    letter-spacing: 1.6px;

    @media screen and (min-width:768px) {
      margin-bottom: 10px;
    }
  }

  .product__meta {
    display: flex;
    justify-content: space-between;
    width: 100%;
  }

  .product__meta-info {
    max-width: 200px;
    margin-right: 10px;
  }

  .close {
    position: absolute;
    top: 15px;
    right: 43px;
    width: 38px;
    height: 38px;
    z-index: 20;
    cursor: pointer;

    .icon {
      width: 38px;
      height: 38px;

      path {
        fill: $primary;
      }
    }

    @include breakpoint(extra-small) {
      width: 29px;
      height: 29px;
      top: 10px;
      right: 13px;

      .icon {
        width: 29px;
        height: 29px;
      }
    }
  }

  .errors {
    max-width: 100%;
    margin: 0;
    padding: 24px 30px;
    @include breakpoint( small ) {
      padding: 15px 20px;
    }

    .close {
      .icon {
        stroke: #721c24;
      }
    }
  }

  .product__gift-promotion-sold-out {
    display: none;
    margin: 10px 0 0;
  }
}

