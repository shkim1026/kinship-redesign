//typography: base styles for fonts, p, a, standard h1, h2 ...

body {
    font-size: 14px;
    font-family: $basefont;
    font-weight: 600;
}


.animate-fade-in-up {
	opacity: 0;
	transform: translateY(40px);
	transition-property: opacity, transform;
	transition-duration: 0.75s;

	&.animated {
		opacity: 1;
		transform: translateY(0);
	}
}

@for $i from 1 through 10 {
	.animate-delay-#{$i}00 {
		transition-delay: 0.1s * $i;
	}
}

@for $i from 1 through 10 {
	.animate-speed-#{$i}00 {
		transition-duration: 0.1s * $i;
	}
}

* {
	box-sizing: border-box;
	outline: none;
}

html,
body {
	background-color: #fff;
}

body {
	display: flex;
	flex-direction: column;
	min-height: 100vh;
	background-color: #fff;

	&::before {
		content: '';
		display: block;
		position: fixed;
		top: 0;
		right: 0;
		bottom: 0;
		left: 0;
		background: white;
		overflow: hidden;
		width: 100vw;
		height: 100vh;
		z-index: 9999;
		opacity: 0;
		visibility: hidden;
		transition: .4s;
		pointer-events: none;
	}

	&.cart__open {
		position: relative;

		&::before {
			position: absolute;
			top: 0;
			right: 0;
			bottom: 0;
			left: 0;
			width: 100%;
			height: 100%;
			opacity: .9;
			visibility: visible;
			pointer-events: auto;
		}
	}
}

.content {
	flex: 1 0 auto;
}


svg.icon {
	width: 20px; //  can be remove, when start styling icons
}

.visually-hidden {
	position: absolute;
	clip: rect(0 0 0 0);
	width: 1px;
	height: 1px;
	margin: -1px;
}

picture {
	img {
		filter: blur(0);
		transition-property: filter;
		transition-duration: 0.3s;
	}
}

.lazy {
	img {
		// width: 100%; // may be need remove/rework this solution
		filter: blur(10px);
	}
}

.hide {
	display: none;
}

.text-center {
	text-align: center;
}

.align-center {
	align-items: center;
}

.self-center {
	align-self: center;
}

.small--hide {
	@media (max-width: 991px) {
		display: none !important;
	}
}

.medium-up--hide {
	@media (min-width: 992px) {
		display: none !important;
	}
}

.medium-down--hide {
	@media (max-width: 1099px) {
		display: none !important;
	}
}

.large-up--hide {
	@media (min-width: 1100px) {
		display: none !important;
	}
}

