		display: none;
	}

	.rc_label__delivery {
		display: none;
	}

	div.rc_block__type__options {
		display: none;
	}

	.rc_select__frequency {
		-webkit-appearance: none;
		appearance: none;
		display: block;
		width: 100%;
		height: 65px;
		padding-left: 65px;
		padding-right: 50px;
		background-color: #fff;
		background-image: url('icon-recycle.svg'), url('icon-arrow-down.svg');
		background-position: 25px center, calc(100% - 30px) center;
		background-repeat: no-repeat;
		border: 2px solid $primary;
		border-radius: 40px;
		//text-align-last: center;
		letter-spacing: 0.89px;
		font-weight: 400;
		font-size: 16px;
		color: $primary;
		cursor: pointer;
		padding-bottom: 3px;

		option {
			width: 100%;
		}
	}
	// ReCharge end

	.product__form {

		label.gift-select_label {
			color: currentColor;
		}

		label.gift-select_label {
			//margin-left: 6px;
			font-size: 12px;
			letter-spacing: 1px;
			white-space: normal;
			line-height: 1.4 !important;
			text-align: left;
			-webkit-tap-highlight-color: rgba(0,0,0,0);
		}

		.gift-select_label.gift-select_label__deliver_every {
			width: 100%;
			margin-left: 0;
			padding-left: 0;
			padding-top: 15px;

			.gift_select:not(.gift_select__frequency) {
				display: block;
			}
		}

		.gift_select_label__delivery {
			display: none;
		}

		.gift_select__frequency {
			-webkit-appearance: none;
			appearance: none;
			display: block;
			width: 100%;
			height: 65px;
			padding-left: 65px;
			padding-right: 50px;
			background-color: #fff;
			background-image: url('icon-recycle.svg'), url('icon-arrow-down.svg');
			background-position: 25px center, calc(100% - 30px) center;
			background-repeat: no-repeat;
			border: 2px solid $primary;
			border-radius: 40px;
			//text-align-last: center;
			letter-spacing: 0.89px;
			font-weight: 400;
			font-size: 16px;
			color: $primary;
			cursor: pointer;
			padding-bottom: 3px;

			option {
				width: 100%;
			}
		}

		label.gift-card-label {
			position: relative;
			display: block;
			padding-top: 0;
			margin-top: 15px;

			.gift_select__frequency {
				position: relative;
				padding-left: 80px;
				background-image: url('icon-arrow-down.svg');
				background-position: calc(100% - 30px) center;
				margin: 10px 0;
				text-align-last: auto;
				@media (max-width: 1550px) {
					height: 50px;
				}
				@include breakpoint(extra-small) {
					font-weight: bold;
				}
				@media (max-width: 769px) {
					height: 55px;
				}
				@media screen and (max-width: 360px) {
					padding-left: 50px;
					padding-right: 45px;
					background-size: 20px, 11px;
					font-size: 14px;
				}
			}
			.gift-card-variant {
				letter-spacing: 0.89px;
				font-weight: bold;
				font-size: 16px;
				color: $primary;
				position: absolute;
				top: 50%;
				left: 20px;
				transform: translateY(-50%);
				z-index: 2;
			}
		}
	}

	@media (max-width: 1550px) {
		.rc_select__frequency,
		.gift_select__frequency {
			height: 50px;
			text-indent: 40px;
		}
	}

	@media (min-width: 1280px) and (max-height: 710px) and (max-width: 1550px) {
		padding-top: 35px;
		padding-bottom: 40px;
		padding-left: 20px;
		padding-right: 20px;
		transition-property: transform;
		transition-duration: 0.4s;

