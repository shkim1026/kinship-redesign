		}

		.buystack__radio-wrapper {
			margin-top: 10px;
		}

		.product-action {
			margin-top: 15px;
		}

		.buystack__save-price {
			margin-top: 15px;
		}

		// Recharge
		.rc_select__frequency {
			height: 50px;
		}
		// Recharge end
	}

	@media (max-width: 1279px) {
		position: relative !important;
		transform: none;
		bottom: auto !important;

		&.buystack--offset {
			transform: none;
		}
	}

	@include breakpoint(small) {
		margin-top: -85px;
	}

	@include breakpoint(extra-small)  {
		padding-top: 27px;
		padding-right: 20px;
		padding-bottom: 35px;
		padding-left: 20px;

		.buystack__promo-message {
			margin-bottom: 10px;
			font-size: 10px;
		}

		.buystack__title {
			margin-bottom: 9px;
			font-size: 24px;
			line-height: 1.08;
		}

		.buystack__reviews {
			margin-top: 12px;

			.spr-starrating {
				font-size: 8px;
			}
		}

		.buystack__weight {
			line-height: 1.58;
			letter-spacing: 1px;
		}

		.buystack__description {
			margin-top: 1px;
			line-height: 1.5;
			letter-spacing: 0.5px;
		}

		.buystack__preorder-text {
			margin-top: 20px;
		}

		.product-action {
			margin-top: 15px;
		}

		//.buystack__radio-group {
		//	&:first-child {
		//		width: 58%;
		//	}
		//
		//	&:last-child {
		//		width: 42%;
		//	}
		//}

		.buystack__radio-wrapper {
			margin-top: 10px;
		}

		.buystack__save-price {
			margin-top: 20px;
		}

		// ReCharge
		.rc_select__frequency {
			height: 55px;
			background-position: 18px center, calc(100% - 18px) center;
			font-size: 14px;
			font-weight: bold;
		}
		// ReCharge end
	}

	@media screen and (max-width: 360px) {
		// ReCharge
		.rc_select__frequency {
			padding-left: 50px;
			padding-right: 45px;
			background-size: 20px, 11px;
			font-size: 14px;
		}
		// ReCharge end
	}
}

