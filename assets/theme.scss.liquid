		position: absolute;
		left: 15px;
		bottom: 68px;
		display: flex;
		align-items: center;
		height: 65px;
		padding: 22px 34px;
		background-color: #fff;
		border-radius: 37.5px;

		.slick-arrow {
			width: 21px;
			height: 21px;
			padding: 0;
			background-color: transparent;

			svg path {
				fill: $primary !important;
			}

			&.slick-prev {
				order: 0;
				transform: scale(-1);
			}

			&.slick-next {
				order: 2;
			}
		}

		.slick-dots {
			order: 1;
			margin: 0 7px;
		}
