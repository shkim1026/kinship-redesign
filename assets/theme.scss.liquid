.product-details {
	background-color: #eef5fc;

	.product-details__container {
		position: relative;
		padding-top: 75px;
		padding-bottom: 56px;
		padding-left: 120px;
	}

	.product-details__label {
		position: absolute;
		top: 0;
		left: 0;
	}

	.product-details__tabs {
		display: flex;
		position: relative;
		margin-bottom: 53px;
	}

	.product-details__content {
		position: relative;
	}

	.product-details__accordion-wrapper {
		display: none;
	}

	.product-details__tab {
		display: inline-block;
		margin: 0 19px;
		font-size: 16px;
		font-weight: 800;
		letter-spacing: 1.54px;
		text-transform: uppercase;
		text-decoration: none;
		color: $primary;

		&.active {
			text-decoration: underline;
		}
	}

	.product-details__triangle {
		display: none;
		position: absolute;
		z-index: 2;
		top: 0;
		width: 0;
		height: 0;
		margin: 0 19px;
		border-style: solid;
		border-width: 0 14px 12px 14px;
		border-color: transparent transparent #ffffff transparent;
		transition-property: left;
		transition-duration: 0.4s;
		transform: translateY(-100%) translateX(-50%);
	}

	// Ingredients block
	.product-details__ingredient {
		display: flex;

		+ .product-details__ingredient {
			margin-top: 25px;
		}
	}

	.product-details__ingredient-icon {
		flex-shrink: 0;
		align-self: flex-end;
		width: 48px;
		height: 48px;
		margin-right: 26px;
		margin-bottom: 18px;
	}

	.product-details__ingredient-text {
		position: relative;
		max-width: 517px;
		background-color: #fff;
		flex-grow: 1;
		flex-basis: 120px;
		padding-top: 19px;
		padding-bottom: 19px;
		padding-left: 50px;
		padding-right: 37px;
		border-radius: 10px;
		font-size: 12px;
		line-height: 1.92;
		color: #3c3c3c;
		letter-spacing: 0.5px;

		&::before {
			content: "";
			position: absolute;
			right: 100%;
			bottom: 23px;
			width: 0;
			height: 0;
			border-style: solid;
			border-width: 8px 9px 8px 0;
			border-color: transparent #fff transparent transparent;
		}

		a {
			color: #3c3c3c;
			&:hover {
				text-decoration: none;
			}
		}
	}

	.product-details__ingredient__links {
		margin-top: 50px;
		padding-left: 55px;
		font-size: 14px;
		letter-spacing: 1px;
		color: #3c3c3c;

		a {
			color: #3c3c3c;
			&:hover {
				text-decoration: none;
			}
		}
	}

	.product-details__ingredient-links-text {

	}

	.product-details__popup-link {
		margin-right: 32px;

		&:last-child {
			margin-right: 0;
		}
	}
	// Ingredients block end


	// Feels
	.product-details__feels {
		display: flex;
		align-items: flex-start;
	}

	.product-details__feels-content {
		position: relative;
		max-width: 483px;
		width: 100%;
		background-color: #fff;
		padding-top: 13px;
		padding-bottom: 5px;
		padding-left: 35px;
		padding-right: 35px;
		border-radius: 10px;

		&::before {
			content: "";
			position: absolute;
			right: 100%;
			bottom: 85px;
			width: 0;
			height: 0;
			border-style: solid;
			border-width: 14px 14px 14px 0;
			border-color: transparent #ffffff transparent transparent;

		}

	}

	.product-details__feels-image {
		flex-shrink: 0;
		margin-right: 30px;
		width: 199px;
		height: 199px;
		margin-top: auto;
		margin-bottom: 65px;
		border-radius: 50%;
		object-fit: cover;
		object-position: 50% 50%;
	}

	.product-details__feels-item {
		display: flex;
		padding-top: 20px;
		padding-bottom: 23px;

		+ .product-details__feels-item {
			border-top: solid 1px #e1e1e1;
		}
	}

	.product-details__feels-title {
		width: 45%;
		font-size: 14px;
		font-weight: bold;
		text-transform: uppercase;
		color: $primary;
		letter-spacing: 1px;
		line-height: 1.64;
	}

	.product-details__feels-text {
		width: 55%;
		padding-left: 23px;
		font-size: 12px;
		letter-spacing: 0.5px;
		line-height: 1.58;
		text-align: right;
	}
	// Feels end


	// Steps
	.product-details__steps {
		max-width: 715px;
		width: 100%;
		max-height: 484px;
		overflow-y: auto;
		padding-top: 50px;
		padding-left: 50px;
		padding-right: 80px;
		padding-bottom: 35px;
		background-color: #fff;
		border-radius: 10px;

		@include scroll-bar;
	}

