.product-recommendations {
    padding-top: 105px;
    padding-bottom: 120px;
    background-color: #e2edf7;

    .product-recommendations__title {
        margin-top: 0;
        margin-bottom: 50px;
        font-size: 60px;
        font-weight: bold;
        letter-spacing: 6px;
        line-height: 1.25;
        text-align: center;
        text-transform: uppercase;
        color: $primary;
    }

    .product-recommendations__row {
        display: grid;
        grid-gap: 30px;
        grid-template-columns: repeat(3, minmax(300px, 1fr));

        &.slick-initialized {
            display: block;
        }

        &.hide-dots {
            .slick-dots {
                display: none;
            }
        }

        .slick-track {
            display: flex;
        }

        .slick-dots {
            position: absolute;
            bottom: -55px;
            left: 50%;
            transform: translateX(-50%);
        }
    }

    .product-recommendations__product {
        padding: 0 15px;
        height: auto;

        .product-card {
            height: 100%;
        }
    }

    @include breakpoint(medium) {
        .product-recommendations__row {
            grid-gap: 20px;
        }
    }

    @include breakpoint(small) {
        .product-recommendations__title {
            font-size: 45px;
            letter-spacing: 4px;
        }

        .product-recommendations__row {
            position: relative;
            display: block;

            &.hide-dots {
                .slick-dots {
                    display: inline-flex;
                }
            }
        }

        .product-recommendations__product {
            padding-left: 5px;
            padding-right: 5px;

            .product-card {
                max-width: 400px;
                margin-left: auto;
                margin-right: auto;
            }
        }
    }

    @include breakpoint(extra-small) {
        padding-top: 55px;
        padding-bottom: 85px;

        .product-recommendations__title {
            margin-bottom: 32px;
            font-size: 32px;
            letter-spacing: 3px;
        }
