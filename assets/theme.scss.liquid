.join-us {
    position: relative;
    padding-bottom: 40px;
    background-size: cover;
    background-repeat: no-repeat;
    background-position: center;
    text-align: center;

    .join-us__background-image  img{
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        object-fit: cover;
        object-position: center;
    }

    .join-us__k-letter {
        position: relative;
        margin-bottom: 35px;

        svg,
        .join-us__k-image {
            width: 100%;
            max-width: 106px;
            margin-top: -30px;
        }
    }

    .join-us__container {
        position: relative;
        width: 100%;
        max-width: 448px;
        margin-left: auto;
        margin-right: auto;
        padding-left: 12px;
        padding-right: 12px;
    }

    .join-us__heading {
        margin-bottom: 10px;
        font-weight: bold;
        letter-spacing: 3.9px;
    }

    .join-us__subheading {
        margin-bottom: 30px;
        line-height: 1.5;
        letter-spacing: 1px;
        font-weight: normal;
        font-size: 14px;
    }

    .join-us__fields {
        max-width: 350px;
        margin-bottom: 20px;
        margin-left: auto;
        margin-right: auto;

        .input-group {
            margin-bottom: 15px;

            &:first-of-type {
                position: relative;

                &:after {
                    content: 'OR';
                    position: absolute;
                    z-index: 2;
                    top: calc(100% - 13px);
                    left: 50%;
                    display: block;
                    width: 40px;
                    height: 40px;
                    border-radius: 50%;
                    text-align: center;
                    line-height: 40px;
                    letter-spacing: 1px;
                    font-size: 12px;
                    font-weight: bold;
                    transform: translateX(-50%);
                }

                &.input-group--no-after {
                    &::after {
                        display: none;
                    }
                }
            }

            &.display--none {
                &:first-of-type {
                    &:after {
                        display: none;
                    }
                }

                input {
                    display: none;
                }

                label {
                    display: none;
                }
            }
        }

        .button {
            min-width: auto;
            width: 100%;
        }
    }

    .join-us__errors {
        position: relative;
        display: none;
        margin-top: 30px;
        margin-bottom: 25px;
        padding: 10px 30px;
        background-color: #fff;
        border: 3px solid transparent;
        border-radius: 10px;
        text-align: left;

        &:before {
            content: '';
            position: absolute;
            top: -11px;
            left: 50%;
            width: 15px;
            height: 15px;
            display: block;
            background-color: #fff;
            border-top: 3px solid transparent;
            border-right: 3px solid transparent;
            transform: translateX(-50%) rotate(-45deg);
        }
    }

    .join-us__error {
        display: flex;
        margin-top: 10px;
        margin-bottom: 10px;
        line-height: 1.5;
        letter-spacing: 1px;
        font-size: 14px;
        font-weight: bold;

        svg {
            width: 20px;
            height: 20px;
        }

        p {
            display: inline-block;
            margin-left: 15px;
        }
    }

    .join-us__disclaimer {
        padding: 17px;
        background-color: rgba(255, 255, 255, 0.7);
        border-radius: 10px;
        line-height: 1.5;
        letter-spacing: 1px;
        font-family: $basefont;

        a {
            color: inherit;

            &:hover {
                text-decoration: none;
            }
        }
    }

    @include breakpoint(mobile-landscape) {
        padding-bottom: 108px;

        .join-us__k-letter {
            margin-bottom: 55px;

            svg {
                max-width: 133px;
                margin-top: -35px;
            }
        }

        .join-us__fields {
            max-width: 304px;

            .input-group {
                margin-bottom: 10px;

                &:first-of-type {
                    &:after {
                        top: calc(100% - 11px);
                        width: 31px;
                        height: 31px;
                        line-height: 31px;
                        letter-spacing: 0.67px;
                        font-size: 8px;
                    }
                }
            }
        }

        .join-us__disclaimer {
            padding: 15px 20px;
        }
    }
}

